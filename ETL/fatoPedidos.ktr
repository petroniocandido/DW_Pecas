    <connection>SqlServer</connection>
    <sql>SELECT
	i.QUANTIDADE
,	i.VALOR_UN
, 	i.DATA_COMPRA
,	c.NOME
,	C.SEXO
,	C.DAT_NASCIMENTO
,	C.BAIRRO
,	C.CIDADE
,	C.ESTADO
, 	P.DESCRICAO AS PRODUTO
FROM dsItens as i 
	join dsClientes as c on c.CLIENTE = i.CLIENTE
	join dsProdutos as p on p.PRODUTO = i.PRODUTO
</sql>
